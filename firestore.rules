rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /articles/{docId} {
      allow read: if true;
      allow write: if false;
    }
    match /sources/{docId} {
      allow read: if false;
      allow write: if false;
    }
    match /jobs/{docId} {
      allow read: if false;
      allow write: if false;
    }
  }
}
